Algoritmo "ex_09"

var
       volume, raio : real
       
inicio
        //const pi 3.14159
         escreva("Valor do Raio: ")
         leia(raio)

         //processamento

         volume <- 4/3 * 3.14159 * raio ^ 3

         //saída de dados
         escreval("Volume = ", volume:10:5)
fimalgoritmo