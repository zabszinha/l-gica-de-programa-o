Algoritmo "ex_12"

var
       C, F : real

   
inicio

          // entrada de dados

           escreva("A temperatura em fahrenheint é: ")
           leia(F)
           
           // processamento
           
           C <- 5/9 * (F - 32)
           
           // saída
           
           escreval("A temperatura em graus celsius é: ")



fimalgoritmo